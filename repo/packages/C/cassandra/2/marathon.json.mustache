{
  "id": "/{{framework.name}}",
  "cmd": "./scheduler/bin/cassandra-scheduler server ./scheduler/conf/scheduler.yml",
  "instances": 1,
  "cpus": {{framework.cpus}},
"mem": {{framework.memoryMb}},
"ports": [
{{apiPort}}, {{adminPort}}
],
"uris": [
"{{resource.assets.uris.scheduler-zip}}",
"{{resource.assets.uris.server-jre-8u74-linux-x64-tar-gz}}"
],
"healthChecks": [
{
"gracePeriodSeconds": 120,
"intervalSeconds": 30,
"maxConsecutiveFailures": 20,
"path": "/healthcheck",
"portIndex" : 1,
"protocol": "HTTP",
"timeoutSeconds": 20
}
],
"labels": {
"DCOS_PACKAGE_FRAMEWORK_NAME": "{{framework.name}}"
},
"env":{
"JAVA_HOME":"./jre"
,"USER":"{{framework.user}}"
