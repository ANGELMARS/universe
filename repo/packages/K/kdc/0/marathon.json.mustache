{
  "id":"{{service.name}}",
  "instances":1,
  "cpus":1,
  "mem":128,
  "env":{
    "PRINCIPAL_hdfs":"hdfs@LOCAL:secret",
    "PRINCIPAL_hdfs1":"{{users.user1}}",
    "PRINCIPAL_hdfs2":"{{users.user2}}",
    "PRINCIPAL_hdfs3":"{{users.user3}}",
    "PRINCIPAL_tester":"tester@LOCAL:secret",
    "PRINCIPAL_http1":"{{users.user4}}",
    "PRINCIPAL_http2":"{{users.user5}}",
    "PRINCIPAL_http3":"{{users.user6}}",
    "KEYTAB_hdfs":"hdfs@LOCAL:hdfs.keytab",
    "KEYTAB_hdfs1" : "{{users.tab1}}",
    "KEYTAB_hdfs2" : "{{users.tab2}}",
    "KEYTAB_hdfs3" : "{{users.tab3}}",
    "KEYTAB_http1" : "{{users.tab4}}",
    "KEYTAB_http2" : "{{users.tab5}}",
    "KEYTAB_http3" : "{{users.tab6}}",
    "KEYTAB_tester":"tester@LOCAL:tester.keytab"
  },
  "container":{
    "type":"DOCKER",
    "docker":{
      "image":"{{kdc.docker_image}}",
      "network":"BRIDGE",
      "forcePullImage":true,
      "parameters":[
        {
          "key":"publish",
          "value":"88:88/udp"
        },
        {
          "key":"publish",
          "value":"88:88/tcp"
        },
        {
          "key":"publish",
          "value":"8000:8000/tcp"
        }
      ]
    }
  }
}
