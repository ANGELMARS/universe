{
  "id":"{{service.name}}",
  "instances":1,
  "cpus":1,
  "mem":128,
  "env":{
    "PRINCIPAL_hdfs":"hdfs@LOCAL:secret",
    "PRINCIPAL_hdfs1":"hdfs/ip-10-0-3-167.us-west-2.compute.internal@LOCAL:secret",
    "PRINCIPAL_hdfs2":"hdfs/ip-10-0-3-168.us-west-2.compute.internal@LOCAL:secret",
    "PRINCIPAL_hdfs3":"hdfs/ip-10-0-3-169.us-west-2.compute.internal@LOCAL:secret",
    "PRINCIPAL_tester":"tester@LOCAL:secret",
    "PRINCIPAL_http1":"HTTP/ip-10-0-3-167.us-west-2.compute.internal@LOCAL:secret",
    "PRINCIPAL_http2":"HTTP/ip-10-0-3-168.us-west-2.compute.internal@LOCAL:secret",
    "PRINCIPAL_http3":"HTTP/ip-10-0-3-169.us-west-2.compute.internal@LOCAL:secret",
    "KEYTAB_hdfs":"hdfs@LOCAL:hdfs.keytab",
    "KEYTAB_hdfs1" : "hdfs/ip-10-0-3-167.us-west-2.compute.internal@LOCAL:hdfs.keytab",
    "KEYTAB_hdfs2" : "hdfs/ip-10-0-3-168.us-west-2.compute.internal@LOCAL:hdfs.keytab",
    "KEYTAB_hdfs3" : "hdfs/ip-10-0-3-169.us-west-2.compute.internal@LOCAL:hdfs.keytab",
    "KEYTAB_http1" : "HTTP/ip-10-0-3-167.us-west-2.compute.internal@LOCAL:hdfs.keytab",
    "KEYTAB_http2" : "HTTP/ip-10-0-3-168.us-west-2.compute.internal@LOCAL:hdfs.keytab",
    "KEYTAB_http3" : "HTTP/ip-10-0-3-169.us-west-2.compute.internal@LOCAL:hdfs.keytab",
    "KEYTAB_tester":"tester@LOCAL:tester.keytab"
  },
  "container":{
    "type":"DOCKER",
    "docker":{
      "image":"{{kdc.docker_image}}",
      "network":"BRIDGE",
      "forcePullImage":true,
      "parameters":[
        {
          "key":"publish",
          "value":"88:88/udp"
        },
        {
          "key":"publish",
          "value":"88:88/tcp"
        },
        {
          "key":"publish",
          "value":"8000:8000/tcp"
        }
      ]
    }
  }
}
